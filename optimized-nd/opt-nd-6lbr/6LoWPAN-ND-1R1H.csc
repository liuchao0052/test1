      <moteinterface>org.contikios.cooja.mspmote.interfaces.SkyTemperature</moteinterface>
    </motetype>
    <motetype>
      org.contikios.cooja.mspmote.SkyMoteType
      <identifier>sky2</identifier>
      <description>6ln</description>
      <source EXPORT="discard">[CONFIG_DIR]/../6lowpan-nd-6ln/ln.c</source>
      <commands EXPORT="discard">make ln.sky TARGET=sky</commands>
      <firmware EXPORT="copy">[CONFIG_DIR]/../6lowpan-nd-6ln/ln.sky</firmware>
